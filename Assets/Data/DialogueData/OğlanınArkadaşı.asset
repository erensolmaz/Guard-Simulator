      nextNodeID: 4
      autoEndDialogue: 1
      triggerArrest: 1
      triggerRage: 0
      triggerQuest: 0
  - nodeNumber: 3
    npcText: "Tamam abi tam sen de ne asabi \xE7\u0131kt\u0131n. \u015Eu an bla bla
