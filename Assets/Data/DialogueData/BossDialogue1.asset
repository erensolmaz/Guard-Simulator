  endDialogueDelay: 2
  dialogueNodes:
  - nodeNumber: 0
    npcText: "Gel bakal\u0131m evlat, otur. Son yapt\u0131\u011F\u0131n tutuklama
      harikayd\u0131. Tam bekledi\u011Fim gibi. Sen bu i\u015Fi namusunla yap\u0131yorsun,
      biliyorum. Sana yeni bir g\xF6revim olacak kabul edersen?"
    quitAfterText: 0
    choices:
    - choiceText: "Te\u015Fekk\xFCr ederim Amir. G\xF6revimi yap\u0131yorum.  Dinliyorum."
      nextNodeID: 1
      autoEndDialogue: 0
      triggerArrest: 0
      triggerRage: 0
      triggerQuest: 0
      triggerCollectQuest: 0
  - nodeNumber: 1
    npcText: "\xC7evrede \xE7ok uyu\u015Fturucu da\u011F\u0131t\u0131m\u0131 var,
      bu i\u015Fin ucunu bulman\u0131 istiyorum."
    quitAfterText: 0
    choices:
    - choiceText: "\xDCzg\xFCn\xFCm amirim ama bu kadar yo\u011Fun bir gecede basit
        ke\u015Flerle kedi-fare oyunu oynamamal\u0131y\u0131z."
      nextNodeID: 2
      autoEndDialogue: 0
      triggerArrest: 0
      triggerRage: 0
      triggerQuest: 0
      triggerCollectQuest: 0
    - choiceText: "Peki amirim, adam\u0131 bulmaya gidiyorum."
      nextNodeID: 3
      autoEndDialogue: 0
      triggerArrest: 0
      triggerRage: 0
      triggerQuest: 1
      triggerCollectQuest: 0
  - nodeNumber: 2
    npcText: "Peki. Gidebilirsin. Ama unutma, bazen do\u011Fruyu yapmak \xE7ok zor."
    quitAfterText: 1
    choices:
    - choiceText: Tamam gidiyorum.
      nextNodeID: 0
      autoEndDialogue: 1
      triggerArrest: 0
      triggerRage: 0
      triggerQuest: 0
      triggerCollectQuest: 0
  - nodeNumber: 3
    npcText: "Harikas\u0131n evlat! Do\u011Fru se\xE7imi yapt\u0131n. \u0130\u015Fte
